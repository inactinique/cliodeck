@article{azevedo_revolutionizing_2024,
	title = {Revolutionizing {History} {Teaching} with {AI}: {A} {Journey} with {ChatGPT}},
	copyright = {Creative Commons Attribution 4.0 International},
	shorttitle = {Revolutionizing {History} {Teaching} with {AI}},
	url = {https://zenodo.org/doi/10.5281/zenodo.12611809},
	doi = {10.5281/ZENODO.12611809},
	abstract = {This article discusses the integration of artificial intelligence into history education, specifically through the use of OpenAI's ChatGPT. Azevedo, with 25 years of experience in teaching history, elaborates on how technology can enhance human capabilities rather than replace educators.

Azevedo addresses the historical resistance of educators to technology due to insufficient training, and how the recent pandemic has accelerated the adoption of digital tools in education. He introduces an educational activity called "Letters to the Past," where students write letters to historical figures after studying a specific period, such as the French Revolution. This activity enables students to interact with history in a fun and practical manner.

With the introduction of ChatGPT, Azevedo has been able to scale this activity, allowing for personalized responses from historical figures to student letters, thereby increasing interaction and enriching the learning experience. ChatGPT also assists in collectively analyzing the letters to identify common themes and conceptual errors like anachronisms.

The document concludes by highlighting the transformative potential of AI in education and invites educators and technology enthusiasts to explore AI tools in their teaching practices, share experiences, and discover new possibilities for enhancing educational processes.},
	language = {en},
	urldate = {2025-07-21},
	author = {Azevedo, Leandro Villela de},
	month = jul,
	year = {2024},
	note = {Publisher: Zenodo
Version Number: 1.0},
	pages = {4--9},
}

