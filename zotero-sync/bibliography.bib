@article{zhongshili_study_2025,
	title = {A {Study} on the {Difficulties} and {Psychological} {Cognitive} {Mechanism} of {Chinese} {Tone} {Second} {Language} {Acquisition} {Based} on {Phonetic} {Contrastive} {Analysis}},
	volume = {1},
